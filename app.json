{
  "expo": {
    "name": "الطريق إلي القرآن",
    "slug": "way2quran",
    "description": "استماع وتحميل تلاوات للقرآن الكريم من أنحاء العالم الإسلامي",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/logo.png",
    "scheme": "way2quran",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/bg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#374151"
      },
      "package": "com.anasnady.way2quran"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "48a677c9-bb19-490e-b5a3-4aab62d7536b"
      }
    }
  }
}
